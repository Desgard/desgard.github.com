{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "$": true,
    "jQuery": true
  },
  "rules": {
    "quotes": [0, "double"],
    "strict": 0,
    "space-infix-ops": 0,
    "no-extra-boolean-cast": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "camelcase": 1
  }
}
