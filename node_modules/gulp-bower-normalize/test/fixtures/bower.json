{
  "overrides": {
    "normalizeMulti": [
      {
        "dependencies": ["dependency8", "dependency9"],
        "normalize": {
          "font": ["*.ttf", "*.woff2"],
          "css": ["*.css"]
        }
      },
      {
        "dependencies": ["dependency9", "dependency10"],
        "normalize": {
          "javascript": ["*.js"],
          "lessFiles": ["*less"]
        }
      },
      {
        "dependencies": ["dependency9"],
        "normalize": {
          "javascript": ["*.*"]
        }
      }
    ],
    "dependency1": {
      "main": "some.js"
    },
    "dependency2": {
      "main": "some.js",
      "normalize": {
        "javascript": "*.js"
      }
    },
    "dependency3": {
      "main": ["some.js", "some.css"],
      "normalize": {
        "js": "*.js",
        "css": "*.css"
      }
    },
    "dependency4": {
      "main": ["some.js", "some.json"],
      "normalize": {
        "js": ["*.js", "*.json"]
      }
    },
    "dependency5": {
      "main": ["some.js", "some.json"],
      "normalize": {
        "js": "*.js"
      }
    },
    "dependency6": {
      "main": "some/long/path/to/file.js"
    },
    "dependency7": {
      "main": ["some.js", "some/other.js"],
      "normalize": {
        "js": "*.js",
        "js/some": "**/some/*.js"
      }
    },
    "dependency8": {
      "main": "some.js",
      "normalize": {
        "js": ["*.js"]
      }
    }
  }
}
